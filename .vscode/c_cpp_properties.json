{
    "version": 4,
    "configurations": [
        {
            "name": "Mac",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/src/cpp/include",
                "${workspaceFolder}/Lib/eigen-3.4.0",
                "${workspaceFolder}/Lib/highway/hwy",
                "${workspaceFolder}/Lib/libtorch/include",
                "${workspaceFolder}/Lib/libtorch/include/torch/csrc/api/include",
                "/opt/homebrew/include",
                "/usr/local/include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include/c++/v1",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/lib/clang/*/include",
                "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/include"
            ],
            "defines": [
                "USE_TORCH",
                "USE_HIGHWAY",
                "USE_OPENMP",
                "_DEBUG",
                "DEBUG"
            ],
            "macFrameworkPath": [
                "/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX.sdk/System/Library/Frameworks"
            ],
            "compilerPath": "/usr/bin/clang++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "macos-clang-x64",
            "compileCommands": "${workspaceFolder}/build/compile_commands.json",
            "configurationProvider": "ms-vscode.cmake-tools",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${workspaceFolder}/src/cpp/include",
                    "${workspaceFolder}/Lib/eigen-3.4.0",
                    "${workspaceFolder}/Lib/highway/hwy",
                    "${workspaceFolder}/Lib/libtorch/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            }
        }
    ]
}
